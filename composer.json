{
    "name": "wp/triggerplugin",
    "type": "library",
    "description": "This plugin allows you to trigger a Gitlab pipeline from WordPress",
    "keywords": [
		"wordpress", "wp"
	],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "10up/wp_mock": "^0.4.2",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "./src"
        },
        "files": ["./src/wp-trigger-plugin.php"]
    }
}
